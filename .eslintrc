{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
      "react"
  ],
  "env":{
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules":{
    "react/jsx-no-undef":0,
    "react/no-deprecated":1

  }

}